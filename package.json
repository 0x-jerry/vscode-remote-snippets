{
  "name": "vscode-remote-snippets",
  "displayName": "Vscode Remote Snippets",
  "description": "Auto fetch snippets from remote and load it.",
  "version": "0.2.0",
  "license": "MIT",
  "author": {
    "name": "Jerry wang",
    "email": "x.jerry.wang@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/0x-jerry/vscode-remote-snippets/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/0x-jerry/vscode-remote-snippets.git"
  },
  "publisher": "fantasy",
  "engines": {
    "vscode": "^1.41.1"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "remote",
    "snippets"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "resources/logo.png",
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Remote Snippets",
      "properties": {
        "remote-snippets.snippets": {
          "type": "array",
          "description": "Remote snippets",
          "items": {
            "type": "object",
            "properties": {
              "url": {
                "type": "string",
                "description": "Remote snippet url"
              },
              "language": {
                "type": "string",
                "description": "Program language the snippet applied to."
              }
            }
          }
        },
        "remote-snippets.config": {
          "type": "array",
          "description": "Remote snippets config",
          "items": {
            "type": "string"
          }
        }
      }
    },
    "commands": [
      {
        "command": "remote-snippets.refresh",
        "category": "Remote Snippets",
        "title": "Update snippets"
      }
    ],
    "keybindings": [],
    "viewsContainers": {},
    "views": {},
    "menus": {},
    "grammars": [],
    "snippets": []
  },
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r",
    "vscode:prepublish": "tsup src/extension.ts --sourcemap",
    "build": "tsup src/extension.ts --sourcemap",
    "dev": "tsup src/extension.ts --sourcemap --watch",
    "publish": "vsce publish --no-dependencies",
    "release": "x-release"
  },
  "peerDependencies": {
    "vscode": "*"
  },
  "devDependencies": {
    "@0x-jerry/utils": "^1.1.1",
    "@0x-jerry/x-release": "^0.3.2",
    "@types/debounce": "^1.2.1",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^15.14.9",
    "@types/vscode": "^1.41.1",
    "ajv": "^8.10.0",
    "axios": "^0.21.4",
    "conventional-changelog-cli": "^2.2.2",
    "dayjs": "^1.10.7",
    "debounce": "^1.2.1",
    "debug": "^4.3.3",
    "form-data": "^4.0.0",
    "fs-extra": "^10.0.1",
    "https-proxy-agent": "^5.0.0",
    "tsup": "^5.11.11",
    "typescript": "^4.5.5",
    "vsce": "^2.6.7"
  }
}